      <th scope="col">Cargo</th>
      <th scope="col">Correo electrónico</th>
      <th scope="col">N° de teléfono</th>
    </tr>
  </thead>
      <tbody>
      <tr>
        <td><a href="/" class="btn btn-success">Ir a Agenda</a></td>
        <td>{{name}}</td>
        <td>{{surname}}</td>
        <td>{{date}}</td>
        <td>{{position}}</td>
        <td>{{email}}</td>
        <td>{{phone}}</td>
      </tr>
      </tbody>
   </table>